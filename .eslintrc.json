{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "jest",
    "jsx-a11y",
    "react",
    "react-hooks"
  ],
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/jsx-props-no-spreading": "warn"
  }
}
